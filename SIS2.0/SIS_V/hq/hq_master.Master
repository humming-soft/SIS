    <script type="text/javascript">
        function watch(d, m, y) {
            if ($.find('#watch')[0]) {

                $('#watch').countDown({
                    targetDate: {
                        'day': d,
                        'month': m,
                        'year': y,
                        'hour': 24,
                        'min': 00,
                        'sec': 00
                    },
                    omitWeeks: true
                });
                //enter the count down date using the format year, month, day, time: hour, min, sec

                

            }
        }
    </script>

