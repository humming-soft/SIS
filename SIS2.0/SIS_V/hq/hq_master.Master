    <script type="text/javascript">
        function election() {
            var cd, d = 0, m = 0, m1 = 0, y = 0;
            $.ajax({
                type: "POST",
                contentType: "application/json",
                data: {},
                url: '<%=Microsoft.AspNet.FriendlyUrls.FriendlyUrl.Resolve("HQ_Utama.aspx/electionInfo")%>',
                dataType: "json",
                success: function (data) {
                    $(".current_election").each(function () {
                        $(this).text(data.d[0].electionName);
                    });
                    cd = new Date(data.d[0].electionDate);
                    d = cd.getDate();
                    m = cd.getMonth();
                    m1 = ++m;
                    y = cd.getFullYear();
                    watch(d, m1, y);
                },
                error: function (XMLHttpRequest, textStatus, errorThrown) {
                    console.log(errorThrown);
                },
                complete: function (data) {
                    if ($('.day_field .top').html() == "0") $('.day_field').css('display', 'none');
                }
            });
        }
    </script>        
